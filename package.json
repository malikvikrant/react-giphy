    "mini-css-extract-plugin": "^0.9.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.2.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5",
    "rxjs": "^6.5.5",
    "webpack-combine-loaders": "^2.0.4"
  }
